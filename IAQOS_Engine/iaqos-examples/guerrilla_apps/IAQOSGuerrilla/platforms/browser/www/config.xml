<?xml version='1.0' encoding='utf-8'?>
<widget id="online.iaqos.iaqosguerrilla" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>IAQOSGuerrilla</name>
    <description>
        IAQOS Mobile interface
    </description>
    <author email="info@he-r.it" href="https://he-r.it">
        Salvatore Iaconesi
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="Orientation" value="landscape" />
    <preference name="KeepRunning" value="true" />
</widget>
